{
    "version": 2,
    "name": "car-rental",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "X-Frame-Options": "DENY",
                "X-Content-Type-Options": "nosniff",
                "Referrer-Policy": "origin-when-cross-origin",
                "Permissions-Policy": "camera=(), microphone=(), geolocation=()"
            }
        }
    ],
    "functions": {
        "app/**/*.js": {
            "maxDuration": 30
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}